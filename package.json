{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "description": "Portfolio website for Nathan David McWilliams",
  "author": "Nathan McWilliams",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "PINO_LOG_LEVEL=debug next dev | pino-pretty",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "npx prettier --write .",
    "prettier:check": "prettier --check .",
    "optimize-svg": "npx svgo",
    "optimize-svg-staged": "git --no-pager diff --name-only --cached --diff-filter=AMR 'public/*.svg' | xargs -r npx svgo"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "js-yaml": "^4.1.0",
    "next": "^14.1.0",
    "normalize.css": "^8.0.1",
    "pino": "^8.16.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.1.0",
    "svgo": "^3.0.4",
    "typescript": "4.9.5"
  }
}
