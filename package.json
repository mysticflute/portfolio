{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "description": "Portfolio website for Nathan David McWilliams",
  "author": "Nathan McWilliams",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "PINO_LOG_LEVEL=debug next dev | pino-pretty",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "npx prettier --write .",
    "prettier:check": "prettier --check .",
    "optimize-svg": "npx svgo",
    "optimize-svg-staged": "git --no-pager diff --name-only --cached --diff-filter=AMR 'public/*.svg' | xargs -r npx svgo"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "js-yaml": "^4.1.0",
    "next": "13.5.0",
    "normalize.css": "^8.0.1",
    "pino": "^8.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "18.14.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.34.0",
    "eslint-config-next": "13.2.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^9.4.0",
    "prettier": "^2.8.4",
    "svgo": "^3.0.2",
    "typescript": "4.9.5"
  }
}
